@model IEnumerable<AuthenticatedSchoolSystem.Models.Back_End.Post>

<table class="table">
    <tr>
        <th>
            Blog Posts
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.Raw(item.data)
            </td>
        </tr>
    }
</table>